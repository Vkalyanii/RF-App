<mvc:View
    controllerName="ns.rfapp.controller.ASNSCANFIRST"
    xmlns:unified="sap.ui.unified"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:core="sap.ui.core"
    xmlns:layout="sap.ui.layout"
    xmlns:f="sap.ui.layout.form"
    xmlns:smartTable="sap.ui.comp.smarttable"
    xmlns:ndc="sap.ndc"
    xmlns:cssgrid="sap.ui.layout.cssgrid"
    xmlns:vbm="sap.ui.vbm"
    xmlns:controls="sap.viz.ui5.controls"
    xmlns:commons="sap.suite.ui.commons"
    xmlns:simple="sap.gantt.simple"
    xmlns:launchpad="sap.ushell.ui.launchpad"
    xmlns:uxap="sap.uxap"
    xmlns:form="sap.ui.layout.form"
    xmlns="sap.m"
    displayBlock="true"
    xmlns:tnt="sap.tnt"
>
    <tnt:ToolPage id="idToolPageASNSCAN_First">
        <tnt:header>
            <tnt:ToolHeader id="idtntToolHeaderASNSCAN_First">
                <Image
                    id="idGenImageASNSCAN_First"
                    height="1.5rem"
                    class="sapUiSmallMarginBegin"
                    src="https://artihcus.com/assets/img/AG-logo.png"
                />

                <Title
                    id="idTitleIn_ASNSCANFirst"
                    text="Unloading By ASN"
                    class="title"
                    level="H1"
                    titleStyle="Auto"
                />
                <tnt:ToolHeaderUtilitySeparator
                    id="idToolHeaderUtilitySeparatorASNSCAN_First"
                />
                <ToolbarSpacer id="idToolbarSpacerASNSCAN_First">
                    <layoutData>
                        <OverflowToolbarLayoutData
                            id="idOverflowToolbarLayoutData1ASNSCAN_First"
                            priority="NeverOverflow"
                            minWidth="20px"
                        />
                    </layoutData>
                </ToolbarSpacer>
                <Avatar
                    id="idGenAvatar1ASNSCAN_First"
                    displaySize="XS"
                    press="onAvatarPressed"
                    tooltip="Profile"
                />
            </tnt:ToolHeader>
        </tnt:header>

        <tnt:mainContents>
            <VBox
                id="idVBoxASNSCAN_First"
                justifyContent="Center"
                alignItems="Center"
                class="sapUiMediumMarginTop"
            >
                <f:SimpleForm
                    id="idSimpleFormASNSCAN_First"
                    layout="ResponsiveGridLayout"
                    labelSpanL="4"
                    labelSpanM="4"
                    columnsL="2"
                    columnsM="2"
                    width="100%"
                >
                    <VBox id="idVBox02ASNSCAN_First">
                        <HBox id="idHboxIn_ASNSCANFirst">
                            <Label
                                id="idASNLabelIn_ASNSCANFIRST"
                                text="ASN"
                                class="sapUiTinyMarginTop"
                                width="40px"
                            />
                            <!-- Input Field -->
                            <Input
                                id="idASNInputIn_ASNSCANFIRST"
                                placeholder="Enter ASN"
                                width="7rem"
                            />
                        </HBox>

                        <HBox
                            id="idhbox_ASNSCANFirst"
                            class="sapTinyMarginEnd"
                        >
                            <!-- Barcode Scan Button -->
                            <Button
                                id="idGenButtonASNSCAN_First"
                                icon="sap-icon://bar-code"
                                press="Onpressscan"
                                class="sapUiTinyMarginEnd"
                            />
                            <!-- Submit Button -->
                            <Button
                                id="idSubmitButtonIn_ASNSCANFirst"
                                text="Submit"
                                class="sapUiTinyMarginEnd"
                                press="onPressSubmitIn_ASNSCANFirst"
                            />

                            <Button
                                id="idBackButtonIn_ASNSCANFirst"
                                text="Back"
                                class="sapUiTinyMarginEnd"
                                press="OnPressBackIn_ASNSCANFirst"
                            />
                        </HBox>
                    </VBox>
                    <!-- </HBox> -->
                </f:SimpleForm>
                <!-- </VBox> -->
            </VBox>
        </tnt:mainContents>
    </tnt:ToolPage>
</mvc:View>
