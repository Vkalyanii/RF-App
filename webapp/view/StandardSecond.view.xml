 
<mvc:View
    controllerName="ns.rfapp.controller.StandardSecond"
    xmlns:unified="sap.ui.unified"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:core="sap.ui.core"
    xmlns:layout="sap.ui.layout"
    xmlns:f="sap.ui.layout.form"
    xmlns:smartTable="sap.ui.comp.smarttable"
    xmlns:ndc="sap.ndc"
    xmlns:cssgrid="sap.ui.layout.cssgrid"
    xmlns:vbm="sap.ui.vbm"
    xmlns:controls="sap.viz.ui5.controls"
    xmlns:commons="sap.suite.ui.commons"
    xmlns:simple="sap.gantt.simple"
    xmlns:launchpad="sap.ushell.ui.launchpad"
    xmlns:uxap="sap.uxap"
    xmlns:form="sap.ui.layout.form"
    xmlns="sap.m"
    displayBlock="true"
    xmlns:tnt="sap.tnt"
>
    <tnt:ToolPage id="idToolPage_StandardSecond">
       
        <tnt:mainContents>
           
            <ScrollContainer
                id="idScrollContainerIn_StandardSecond"
                width="100%"
                height="100%"
                horizontal="false"
                vertical="true"
                class="sapUiContentPadding"
                visible="true"
            >
              
                <VBox
                    id="idVBoxThirdScreen_StandardSecond"
                    justifyContent="Center"
                    alignItems="Center"
                    class="sapUiMediumMarginTop"
                >
                    <form:SimpleForm
                        id="idSimpleForm4_StandardSecond"
                        layout="ResponsiveGridLayout"
                        labelSpanL="4"
                        labelSpanM="4"
                        columnsL="2"
                        columnsM="2"
                        width="100%"
                    >
                        <VBox id="idVBox02Page3_StandardSecond">
                            <HBox id="idHBox01Page3_StandardSecond">
                                <Text
                                    id="idLabelDelivery_StandardSecond"
                                    text="Delivery"
                                    width="100px"
                                    class="sapUiTinyMarginTop"
                                />
                                <Input
                                    id="idInputDelivery_StandardSecond"
                                    width="14rem"
                                    value=""
                                    editable="false"
                                    maxLength="35"
                                    submit="onSubmit"
                                    
                                />
                               
                            </HBox>
                            <HBox id="idHBox02Page_StandardSecond">
                                <Text
                                    id="idLabelWTs_StandardSecond"
                                    text="Partner"
                                    width="100px"
                                    class="sapUiTinyMarginTop"
                                />
                                <Input
                                    id="idInputNohu_StandardSecond"
                                      width="7rem"
                                    value=""
                                    editable="false"
                                       maxLength="10"
                                />
                               
                            </HBox>
                            <HBox id="idHBox03Page_StandardSecond">
                                <Text
                                    id="idLabelName_StandardSecond"
                                    text="Name"
                                      width="100px"
                                    class="sapUiTinyMarginTop"
                                />
                                <Input
                                    id="idInputUnloadIn_WTQBYHU"
                                     width="15rem"
                                    value=""
                                    editable="false"
                                    maxLength="40"
                                />
                                
                            </HBox>
                            <HBox id="idHBox07Page3_StandardSecond">
                                <Text
                                    id="idLabelGRIn_StandardSecond"
                                    text="Whse Door"
                                      width="100px"
                                    class="sapUiTinyMarginTop"
                                />
                                <Input
                                    id="idInputGRIn_StandardSecond"
                                      width="4rem"
                                    value=""
                                    editable="false"
                                    maxLength="4"
                                />
                               
                            </HBox>

                              <HBox id="idHBox07Page_StandardSecond">
                                <Text
                                    id="idLabelWTCreated_StandardSecond"
                                    text="No.of HUs"
                                   width="100px"
                                    class="sapUiTinyMarginTop"
                                />
                                <Input
                                    id="idI1nputWTCreatedIn_StandardSecond"
                                    width="3rem"
                                    value=""
                                    editable="false"
                                    maxLength="3"
                                />
                               
                            </HBox>
                             <HBox id="idHBox07Page32_StandardSecond">
                                <Button
                            id="idHULISTButtonIn_StandardSecond"
                            text="HU List"
                            class="sapUiTinyMarginEnd"
                            press="onPressHuListIn_StandardSecond"
                        />

                          <Button
                            id="idNewHUButtonIn_StandardSecond"
                            text="Enter"
                            class="sapUiTinyMarginEnd"
                            press="onPressEnterIn_StandardSecond"
                        />
                             <Button
                            id="idBackButtonIn_StandardSecond"
                            text="Back"
                            class="sapUiTinyMarginEnd"
                            press="onPressBackBtnIn_StandardSecond"
                        />

                               
                            </HBox>
                        </VBox>
                    </form:SimpleForm>
                </VBox>
            </ScrollContainer>
 
           
     
        </tnt:mainContents>
    </tnt:ToolPage>
</mvc:View>
 
 